{
  "name": "react-container-query-site",
  "version": "1.0.0",
  "description": "Homepage for React Container Query",
  "author": "Daiwei Lu",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "browser-sync": "^2.10.1",
    "classnames": "^2.2.1",
    "highlight.js": "^9.0.0",
    "node-sass": "^3.4.2",
    "react": "^0.14.3",
    "react-container-query": "0.1.0",
    "react-dom": "^0.14.3",
    "react-mixin": "^3.0.3",
    "react-motion": "^0.3.1",
    "webpack": "^1.12.9"
  },
  "scripts": {
    "build:css": "node-sass --output-style expanded scss/index.scss style.css",
    "build:css-prod": "node-sass --output-style compressed scss/index.scss style.css",
    "build:js": "webpack",
    "build:js-prod": "webpack --config webpack.config.production.js",
    "build": "npm run build:css && npm run build:js",
    "build-prod": "npm run build:css-prod && npm run build:js-prod",
    "prestart": "npm run build:css",
    "start": "browser-sync start --no-notify --server --no-ghost-mode --files='*.js, *.css, *.html' & webpack -w & node-sass --output-style expanded -w scss/index.scss style.css"
  }
}
